<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/bluetoothItem">
        <bitmap android:gravity="top|center" android:src="@drawable/ic_sound_bluetooth_holo_dark_hline" />
    </item>
    <item android:id="@id/handsetItem">
        <bitmap android:gravity="top|center" android:src="@drawable/ic_sound_handset_holo_dark_hline" />
    </item>
    <item android:id="@id/speakerphoneItem">
        <bitmap android:gravity="top|center" android:src="@drawable/ic_sound_speakerphone_holo_dark" />
    </item>
    <item android:id="@id/soundItem">
        <bitmap android:gravity="top|center" android:src="@drawable/ic_sound_holo_dark" />
    </item>
</layer-list>